{
  "expo": {
    "name": "NewsUp",
    "slug": "newsUp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.megtrix.newsup"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.megtrix.newsup",
      "versionCode":1,
      "enableDangerousExperimentalLeanBuilds":true
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "description": "News Up Mobile Application"
  }
}
